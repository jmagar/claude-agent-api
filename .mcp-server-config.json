{
  "mcpServers": {
    "test-app-server": {
      "type": "stdio",
      "command": "node",
      "args": ["/path/to/test-server.js"],
      "env": {
        "TEST_VAR": "from_application_config"
      }
    },
    "disabled-app-server": {
      "type": "stdio",
      "command": "node",
      "args": ["/path/to/disabled.js"],
      "enabled": false
    }
  }
}
